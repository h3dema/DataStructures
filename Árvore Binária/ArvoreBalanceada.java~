/**
 * classe auxiliar que implementa o nó da árvore binária
 */
class No<T> {
  T val;
  No esq;
  No dir;
  
  No(T x) {
    val = x;
  }
}



public class Arvore<T> {

  No<T> raiz;
  
  Arvore() {
    raiz = null;
  }

  No<T> inserir(T valor) {
    No<T> x = new No<T>(valor);
    
    return x;
  }
  
}